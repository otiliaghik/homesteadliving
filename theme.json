	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000"
				},
				{
					"name": "Green",
					"slug": "green",
					"color": "#2a3a2f"
				},
				{
					"name": "Gold",
					"slug": "gold",
					"color": "#b48126"
				},
				{
					"name": "Orange",
					"slug": "orange",
					"color": "#8c3e0d"
				},
				{
					"name": "Graphite",
					"slug": "graphite",
					"color": "#39352d"
				},
				{
					"name": "Gray",
					"slug": "gray",
					"color": "#ebebeb"
				},
				{
					"name": "Tan",
					"slug": "tan",
					"color": "#fbf9f5"
				},

				{
					"name": "White",
					"slug": "white",
					"color": "#fff"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"one": 1,
				"heading": 1.2,
				"medium": 1.5,
				"body": 1.75
			},
			"spacing": {
				"outer": "30px"
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Cardo\", serif",
