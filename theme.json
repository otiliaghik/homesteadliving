				"typography": {
					"fontSize": "var(--wp--preset--font-size--max-36)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"style": "solid",
					"width": "1px"
				},
				"spacing": {
					"margin": {
						"bottom": "30px"
					},
					"padding": {
						"top": "30px",
						"right": "30px",
						"bottom": "30px",
						"left": "30px"
