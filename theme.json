					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--green)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"right": "25px",
						"bottom": "10px",
						"left": "25px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/column": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": "0px"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "10px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "30px",
						"left": "0"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "30px"
					}
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
